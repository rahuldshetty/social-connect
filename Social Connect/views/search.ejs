<!DOCTYPE html>
<html>
<head>

    <meta name="viewport" content="width=device-width,initial-scale=1,maximum-scale=1,user-scalable=no">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="HandheldFriendly" content="true">
    <title>Home</title>
    <link rel="stylesheet" type="text/css" media="screen" href="../assets/search.css" />
    <link rel="stylesheet" type="text/css" media="screen" href="https://fonts.googleapis.com/css?family=Pacifico" />
    <link rel="stylesheet" type="text/css" media="screen" href="https://fonts.googleapis.com/css?family=Cabin" />
</head>

<body>


   <div class="topnav">
	  <a href="/home/" class="navitem1">Home</a>
      <a href="/home/cpost" class="navitem1">Create Post</a>
      
      <form method="POST" action="/home/search">
          <input type="text" name="sname" class="searchbox" placeholder="Search here...">
          <input type="submit" value="Search" class="searchBtn">
      </form>

     <a href="/auth/logout" class="navitem2">Log Out</a>
      <a href="/home/profile" class="navitem2">Profile</a>
      <a href="/home/friends" class="navitem2">Friends</a>
      <a href="/home/requests" class="navitem2">Requests</a>
    </div>

    <div class="container">
    <% for(var i=0;i < users.length ; i++) { %>
        <a href="/home/profile/<%=users[i].uid%>" class="nostyle">
        <div class="profileContainer">
            <div class="postHeader">
                <img src="<%=userimages[i]%>" class="smallprofileImg" style="background:rgba(0,0,0,0.5);">    
                <b><p class="uname"><%= users[i].name %></p></b>
                <p class="uid">  <%= users[i].uid  %> </p>
            </div>
        </div>
        </a>
    <% } %>
   
    </div>


</body>
</html>